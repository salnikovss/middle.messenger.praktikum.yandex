    "eslint": "8.23.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "express": "4.18.1",
    "handlebars": "^4.7.7",
    "nanoid": "^4.0.0",
    "parcel": "2.7.0",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "sass": "1.54.9",
    "stylelint": "14.12.0",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard-scss": "5.0.0",
    "stylelint-scss": "4.3.0",
    "typescript": "4.8.3"
  }
